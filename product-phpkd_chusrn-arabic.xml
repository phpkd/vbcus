		<template name="phpkd_chusrn_modifyusername" templatetype="template" date="1206377378" username="PHPKD" version="1.0.0"><![CDATA[<script type="text/javascript" src="clientscript/vbulletin_md5.js?v=$vboptions[simpleversion]"></script>
<script type="text/javascript">
function verify_usernames(password, username1, username2)
{
	// do various checks, this will save people noticing mistakes on next page
	if (password.value == '')
	{
		alert('$vbphrase[phpkd_chusrn_fill_out_password_field]');
		return false;
	}
	else if (username1.value == '' || username2.value == '')
	{
		alert('$vbphrase[phpkd_chusrn_fill_out_both_username_fields]');
		return false;
	}
	else if (username1.value != username2.value)
	{
		alert('$vbphrase[phpkd_chusrn_entered_usernames_do_not_match]');
		return false;
	}
	else
	{
		var junk_output;
		md5hash(document.forms.changeusername.currentpassword, document.forms.changeusername.currentpassword_md5, junk_output, $show[nopasswordempty]);
		return true;
	}
	return false;
}
</script>

